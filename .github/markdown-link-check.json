{
    "ignorePatterns": [
        {
            "pattern": "^http://localhost"
        },
        {
            "pattern": "^https://localhost"
        },
        {
            "pattern": "^http://127.0.0.1"
        },
        {
            "pattern": "^https://127.0.0.1"
        }
    ],
    "replacementPatterns": [
        {
            "pattern": "^/",
            "replacement": "https://github.com/yourusername/rechain-vc-lab/blob/main/"
        }
    ],
    "httpHeaders": [
        {
            "urls": [
                "https://github.com"
            ],
            "headers": {
                "Accept": "text/html,application/xhtml+xml,application/xml;q=0.9,*/*;q=0.8"
            }
        }
    ],
    "timeout": "20s",
    "retryOn429": true,
    "retryCount": 3,
    "fallbackRetryDelay": "1000ms",
    "aliveStatusCodes": [
        200,
        206
    ]
}