// REChain VC Lab iOS Smoke Test Configuration
// This file contains smoke test-specific build configuration settings

// Include base configuration
#include "Config.xcconfig"

// Smoke Test-specific overrides
FLUTTER_BUILD_MODE = profile
GCC_OPTIMIZATION_LEVEL = s
GCC_FAST_MATH = YES
ENABLE_BITCODE = NO
ENABLE_TESTABILITY = NO
DEBUG_INFORMATION_FORMAT = dwarf-with-dsym

// Swift Smoke Test Settings
SWIFT_OPTIMIZATION_LEVEL = -O
SWIFT_COMPILATION_MODE = wholemodule

// Code Signing Smoke Test
CODE_SIGN_IDENTITY = Apple Development
