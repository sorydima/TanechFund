// Smoke Test Architecture
ARCHS = arm64
VALID_ARCHS = arm64 armv7 armv7s

// Smoke Test Flutter Settings
FLUTTER_BUILD_NAME = 1.0.0-smoke-test
FLUTTER_BUILD_NUMBER = 1

// Smoke Test App Configuration
PRODUCT_NAME = REChain VC Lab Smoke Test
