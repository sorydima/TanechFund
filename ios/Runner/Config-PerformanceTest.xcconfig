// REChain VC Lab iOS Performance Test Configuration
// This file contains performance test-specific build configuration settings

// Include base configuration
#include "Config.xcconfig"

// Performance Test-specific overrides
FLUTTER_BUILD_MODE = profile
GCC_OPTIMIZATION_LEVEL = s
GCC_FAST_MATH = YES
ENABLE_BITCODE = NO
ENABLE_TESTABILITY = NO
DEBUG_INFORMATION_FORMAT = dwarf-with-dsym

// Swift Performance Test Settings
SWIFT_OPTIMIZATION_LEVEL = -O
SWIFT_COMPILATION_MODE = wholemodule

// Code Signing Performance Test
CODE_SIGN_IDENTITY = Apple Development
CODE_SIGN_STYLE = Automatic

// Performance Test Build Settings
IPHONEOS_DEPLOYMENT_TARGET = 12.0
TARGETED_DEVICE_FAMILY = 1,2
SUPPORTED_PLATFORMS = iphoneos iphonesimulator

// Performance Test Architecture
ARCHS = arm64
VALID_ARCHS = arm64 armv7 armv7s

// Performance Test Flutter Settings
FLUTTER_BUILD_NAME = 1.0.0-performance-test
FLUTTER_BUILD_NUMBER = 1

// Performance Test App Configuration
PRODUCT_NAME = REChain VC Lab Performance Test
PRODUCT_BUNDLE_IDENTIFIER = com.rechain.vc.performance-test
