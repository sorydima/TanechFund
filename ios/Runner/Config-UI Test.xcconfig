// REChain VC Lab iOS UI Test Configuration
// This file contains UI test-specific build configuration settings

// Include base configuration
#include "Config.xcconfig"

// UI Test-specific overrides
FLUTTER_BUILD_MODE = profile
GCC_OPTIMIZATION_LEVEL = s
GCC_FAST_MATH = YES
ENABLE_BITCODE = NO
ENABLE_TESTABILITY = NO
DEBUG_INFORMATION_FORMAT = dwarf-with-dsym

// Swift UI Test Settings
